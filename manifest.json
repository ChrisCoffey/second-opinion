{
	"manifest_version": 2,
	"name": "Second Opinion",
	"description": "Adds Yelp reviews & a link to yelp for OpenTable reviews",
	"version": "0.0.0.1",
	"permissions":[
		"tabs","<all_urls>"
	],
	"content_scripts":[
		{
			"matches": ["http://*/*"],
			"js": ["zepto.min.js", "replacer.js"],
			"run_at": "document_end",
			"all_frames": true
		}
	]
}